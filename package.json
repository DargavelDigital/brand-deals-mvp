{
  "name": "hyper-by-hype-swagger",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:webpack": "NEXT_SKIP_TURBOPACK=1 next dev",
    "prebuild": "node scripts/verify-prisma-build.mjs",
    "build": "node scripts/prisma-generate.mjs && next build",
    "build:netlify": "node scripts/verify-db-url.ts && next build",
    "postinstall": "prisma generate",
    "clean:prisma": "rimraf node_modules/.prisma",
    "verify:deps": "node scripts/ci/verify-deps.mjs",
    "ci:check-env": "node scripts/ci/check-env-usage.mjs",
    "build:static": "pnpm run build",
    "start": "next start",
    "lint": "eslint",
    "lint:css": "stylelint \"src/**/*.{css,ts,tsx}\" --allow-empty-input",
    "lint:tokens": "node scripts/guards/no-raw-colors.js",
    "lint:styles": "node scripts/guards/no-raw-styles.mjs",
    "lint:ui": "pnpm run lint && pnpm run lint:css && pnpm run lint:tokens && pnpm run lint:styles",
    "check:ui": "node scripts/ui-guard-check.mjs",
    "test:ui": "playwright test -c tests/ui/playwright.config.ts",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:demo": "tsx prisma/seed-demo.ts",
    "db:reset": "pnpm prisma migrate reset --force",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:repair": "prisma migrate resolve --rolled-back 20250829190446_epic19_deal_tracker",
    "db:manual:fix": "node scripts/fix-production-db.mjs",
    "test:e2e:demo:setup": "cross-env DEMO_MODE=true pnpm prisma migrate deploy && pnpm db:seed && pnpm db:seed:demo",
    "test:e2e:demo": "cross-env DEMO_MODE=true playwright test tests/e2e/checklist",
    "test:e2e:real:setup": "cross-env DEMO_MODE=false pnpm prisma migrate deploy && pnpm db:seed",
    "test:e2e:real": "cross-env DEMO_MODE=false playwright test tests/e2e/checklist",
    "test:e2e:matrix": "run-s test:e2e:demo:setup test:e2e:demo test:e2e:real:setup test:e2e:real",
    "test": "vitest",
    "test:run": "vitest run",
    "inspect:react": "pnpm ls react react-dom next",
    "build:start": "next build && next start -p 3000",
    "dev:3000": "next dev -p 3000",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:ci": "start-server-and-test build:start http://localhost:3000 e2e",
    "e2e:a11y": "playwright test tests/a11y",
    "i18n:check": "ts-node --transpile-only scripts/i18n-check.ts",
    "perf:audit": "node k6/run.js audit",
    "perf:matches": "node k6/run.js matches",
    "perf:outreach": "node k6/run.js outreach",
    "migrate:deploy": "prisma migrate deploy",
    "db:seed:test": "ts-node --transpile-only scripts/seed-test.ts",
    "db:smoke": "ts-node --transpile-only scripts/db-smoke.ts",
    "test:ai": "tsx src/ai/evals/runEval.ts",
    "eval": "tsx src/ai/evals/runEval.ts",
    "eval:drift": "tsx src/ai/evals/scores.ts",
    "test:adapt": "vitest __tests__/ai/adapt.test.ts",
    "db:setup:adaptation": "node scripts/setup-epic23-5-adaptation.mjs",
    "dev:seed:contacts": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/dev/qa-contacts-seed.ts",
    "dev:flags:on": "node scripts/dev/set-flags.local.mjs",
    "test:api": "vitest run tests/api/contacts.test.ts --environment node"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@netlify/blobs": "^10.0.11",
    "@prisma/client": "6.15.0",
    "@sendgrid/mail": "^8.1.5",
    "@sparticuz/chromium": "^138.0.2",
    "archiver": "^7.0.1",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.15",
    "dnd-kit": "^0.0.2",
    "handlebars": "^4.7.8",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.541.0",
    "nanoid": "^5.1.5",
    "next": "15.5.0",
    "next-auth": "4.24.11",
    "next-intl": "^4.3.5",
    "next-themes": "^0.4.6",
    "node-vibrant": "^3.2.1-alpha.1",
    "openai": "^5.15.0",
    "pdf-lib": "^1.17.1",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "playwright-core": "^1.55.0",
    "prisma": "6.15.0",
    "puppeteer": "^24.17.0",
    "puppeteer-core": "^24.17.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^2.12.7",
    "resend": "^6.0.1",
    "sanitize-html": "^2.17.0",
    "sonner": "^2.0.7",
    "stripe": "^18.4.0",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "web-push": "^3.6.7",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@types/archiver": "^6.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sanitize-html": "^2.16.0",
    "@types/uuid": "^10.0.0",
    "@types/web-push": "^3.6.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jest": "^30.0.5",
    "k6": "^0.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "start-server-and-test": "^2.0.13",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
