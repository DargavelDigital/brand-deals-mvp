{
  "name": "hyper-by-hype-swagger",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:webpack": "NEXT_SKIP_TURBOPACK=1 next dev",
    "build": "node scripts/prisma-generate.mjs && next build",
    "build:netlify": "node scripts/prisma-generate.mjs && next build",
    "postinstall": "node scripts/prisma-generate.mjs",
    "verify:deps": "node scripts/ci/verify-deps.mjs",
    "build:static": "pnpm run build",
    "start": "next start",
    "lint": "eslint",
    "lint:css": "stylelint \"src/**/*.{css,ts,tsx}\" --allow-empty-input",
    "lint:tokens": "node scripts/guards/no-raw-colors.js",
    "lint:styles": "node scripts/guards/no-raw-styles.mjs",
    "lint:ui": "pnpm run lint && pnpm run lint:css && pnpm run lint:tokens && pnpm run lint:styles",
    "check:ui": "node scripts/ui-guard-check.mjs",
    "test:ui": "playwright test -c tests/ui/playwright.config.ts",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:demo": "tsx prisma/seed-demo.ts",
    "db:reset": "pnpm prisma migrate reset --force",
    "test:e2e:demo:setup": "cross-env DEMO_MODE=true pnpm prisma migrate deploy && pnpm db:seed && pnpm db:seed:demo",
    "test:e2e:demo": "cross-env DEMO_MODE=true playwright test tests/e2e/checklist",
    "test:e2e:real:setup": "cross-env DEMO_MODE=false pnpm prisma migrate deploy && pnpm db:seed",
    "test:e2e:real": "cross-env DEMO_MODE=false playwright test tests/e2e/checklist",
    "test:e2e:matrix": "run-s test:e2e:demo:setup test:e2e:demo test:e2e:real:setup test:e2e:real",
    "inspect:react": "pnpm ls react react-dom next"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^6.14.0",
    "@sendgrid/mail": "^8.1.5",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "dnd-kit": "^0.0.2",
    "dotenv": "^17.2.1",
    "handlebars": "^4.7.8",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "next-auth": "^4.24.11",
    "node-vibrant": "^3.2.1-alpha.1",
    "openai": "^5.15.0",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^24.17.0",
    "puppeteer-core": "^24.17.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^2.12.7",
    "stripe": "^18.4.0",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "jest": "^30.0.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "prisma": "^6.14.0",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
